    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "esbuild": "^0.14.6",
    "jquery": "^3.6.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.0.2",
    "prop-types": "^15.7.2",
